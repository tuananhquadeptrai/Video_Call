# Azure Configuration Environment Variables
# Copy this file to .env and fill in your actual values

# Azure Communication Services
ACS_CONNECTION_STRING=endpoint=https://acs-videocall-app.communication.azure.com/;accesskey=YOUR_ACS_KEY

# Azure Functions
AZURE_FUNCTIONS_URL=https://func-videocall-backend.azurewebsites.net

# Azure SignalR
SIGNALR_CONNECTION_STRING=Endpoint=https://signalr-videocall-chat.service.signalr.net;AccessKey=YOUR_SIGNALR_KEY;Version=1.0;

# Azure Storage
STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=stvideocallapp;AccountKey=YOUR_STORAGE_KEY;EndpointSuffix=core.windows.net

# VideoSDK (Keep as fallback)
VIDEOSDK_TOKEN=YOUR_VIDEOSDK_TOKEN
